//begin footer
+b.FOOTER.footer
	+e.container.container
		+b.logo.footer__logo
			+e.A.link(href='#!')
				+e.IMG.img(src='static/img/logo.svg' alt='Бэби клуб')
			+e.P.descr Бережное развитие интеллекта

		+e.info
			+b.DIV.nav._footer.js-nav
				+e.UL.list
					li: a(href='#benefits') О клубе
					li: a(href='#programs') Программы занятий
					li: a(href='#whyUs') Гарантии
					li: a(href='#testimonials') Отзывы
					li: a(href='#contact') Контакты

			+e.dev
				+e.SPAN Создано в студии
				+e.A.dev-link(href='http://akticom.ru' target='_blank')
					+b.DIV.icon-dev
					+e.SPAN “Актиком”