//begin header
+b.HEADER.header
	+e.container.container
		+b.logo.header__logo
			+e.A.link(href='#!')
				+e.IMG.img(src='static/img/logo.svg' alt='Бэби клуб')
			+e.P.descr Бережное развитие интеллекта

		+e.info
			+e.contacts
				+e.contacts-block._city
					+i('loc')
					+e.SPAN.contacts-text г. Москва
					+e.A(href='#!' data-popup-target='change-city') Сменить город

					+b.popup(data-popup-id='change-city', tabindex='-1', role='dialog')
						+e.container
							+e.close
								+i('close')
							+b.change-city
								+e.top
									+e.H5.title ВЫБЕРИТЕ ГОРОД
