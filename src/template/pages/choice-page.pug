					+e.search-results
						+e.UL
							li.is-active: a(href='#!') Казань

							li: a(href='#!') Калуга

							li: a(href='#!') Кемерово

							li: a(href='#!') Краснодар

				// begin contact map
				+b.contact._section
					+e.inner
						+e.nav
							+e.city
								+e.city-name Казань

							+e.info.js-contact-info
								+e.info-tab
									+e.info-tab-item
										+e.A.info-title.is-active(href='#!') на Вахитова
										+e.info-details
											+e.info-details-field
												+e.P ул. Вахитова, д.8
											+e.info-details-field
												+e.A(href='mailto:msk.beskudnik@babyclub.ru') kzn.vahitova@babyclub.ru
											+e.info-details-field
												+e.A(href='tel:+79154742323') +7 915 4742323
												+e.A(href='tel:+79154742323') +7 915 4742323
											+e.info-details-field
												+e.A.info-btn.btn._green._md(href='#!') Перейти на страницу клуба

									+e.info-tab-item
										+e.A.info-title(href='#!') на Алексеевской
										+e.info-details
