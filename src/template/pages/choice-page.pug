					+e.INPUT.form-input.input(type='text' placeholder='Введите название города' name='' required)
				+e.popular
					+e.popular-title Популярные города:
					+e.A(href='#!') Москва
					+e.A(href='#!') Санкт-Питербург
					+e.A(href='#!') Екатеринбург
					+e.A(href='#!') Краснодар
				+e.search
					+e.UL.search-letters
						+b.LI.is-active
							+e.A(href='#!') А
