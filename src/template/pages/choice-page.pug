											+e.info-details-field
												+e.P Бескудниковский прд, д. 4, корп. 2.
											+e.info-details-field
												+e.A(href='mailto:msk.beskudnik@babyclub.ru') msk.beskudnik@babyclub.ru
											+e.info-details-field
												+e.A(href='tel:+79154742323') +7 915 4742323
											+e.info-details-field
